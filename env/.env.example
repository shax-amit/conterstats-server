# Application Port
PORT=4000

# MongoDB connection string
MONGO_URI=mongodb://username:password@localhost:27017/conterstats

# JWT Secret Key
JWT_SECRET=your_jwt_secret_here

# Cron schedule: Minute 5, every 6 hours (UTC)
SYNC_CRON_SCHEDULE=5 */6 * * *

# TTL-On-Demand in minutes (0 = always fetch)
SYNC_TTL_MINUTES=15